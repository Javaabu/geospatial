{
    "name": "javaabu/geospatial",
    "description": "Adds Geospatial attribute support to Laravel projects",
    "homepage": "https://github.com/Javaabu/geospatial",
    "license": "MIT",
    "keywords": [
        "javaabu",
        "laravel",
        "geospatial"
    ],
    "authors": [
        {
            "name": "Falahabstr",
            "email": "falah.abdulsattar@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.48 || ^11.0 || ^12.0",
        "matanyadaev/laravel-eloquent-spatial": "^4.5"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "orchestra/testbench": "^8.0 || ^9.0 || ^10.0",
        "phpunit/phpunit": "^10.5 || ^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "Javaabu\\Geospatial\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Javaabu\\Geospatial\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Javaabu\\Geospatial\\GeospatialServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
